using SharpArch.Core.DomainModel;
using FashionAde.Core.Clothing;
namespace FashionAde.Core.OutfitEngine
{
    public class PreGarment : Entity
    {
        public PreGarment()
        {
        }

        public PreGarment(int id)
        {
            this.Id = id;
        }

        public virtual Silouhette Silouhette { get; set; }
        public virtual ColorFamily ColorFamily { get; set; }
        public virtual PatternType PatternType { get; set; }
    }
}