using System;
using SharpArch.Core.DomainModel;

namespace FashionAde.Core
{
    public class OutfitDetails : Entity
    {
        private DateTime wornDate;
        private string location;

        public virtual DateTime WornDate
        {
            get { return wornDate; }
            set { wornDate = value; }
        }

        public virtual string Location
        {
            get { return location; }
            set { location = value; }
        }
    }
}