using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace FashionAde.Web.Controllers.MVCInteraction
{
    public class OutfitNotate
    {
        private int outfitSelected;
        private string location;
        private string wornDate;

        public int OutfitSelected
        {
            get { return outfitSelected; }
            set { outfitSelected = value; }
        }

        public string Location
        {
            get { return location; }
            set { location = value; }
        }

        public string WornDate
        {
            get { return wornDate; }
            set { wornDate = value; }
        }
    }
}
