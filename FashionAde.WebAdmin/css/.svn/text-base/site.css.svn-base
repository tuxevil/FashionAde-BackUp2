    background-color:#F4F4F4; 
    display:none; 
    width:610px;     
    padding:15px; 
    text-align:left; 
    border:2px solid #F38333; 
    z-index:9999999;
    
    opacity:1; 
    -moz-border-radius:6px; 
    -webkit-border-radius:6px; 
    -moz-box-shadow: 0 0 50px #ccc; 
    -webkit-box-shadow: 0 0 50px #ccc; 
}

.MyGarment_Modal table
{ 
	float:left;
}

.MyGarment_Modal table td.text 
{
	color: Black;
	font-family: 'Century Gothic';
	font-weight: bold;
	text-align: right;
}

.MyGarment_Tooltip
{ 
	border: solid 1px #000;     
    -moz-box-shadow:0 0 10px #000; 
    -webkit-box-shadow:0 0 10px #000; 
}

.MyGarment_Tooltip_Trash
{
	
	position:relative; 
	width:100%; 
	height:10px; 
	top:0;
}

.MyGarment_Tooltip_Trash img
{
	height:16px;
	width: 16px; 
	top: 2px; 
	right:0;  	
	position:absolute; 
	cursor: pointer;
}

