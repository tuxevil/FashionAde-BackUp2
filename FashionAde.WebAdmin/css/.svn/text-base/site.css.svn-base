/* GRID
----------------------------------------------------------*/
table.grid 
{
	width: 100%;
	border: 1px solid #CCCCCC;
	padding: 2px;
	margin: 0;
}

table.grid th 
{
	background-color: #5C87B2;
	color: #FFFFFF;
	text-align: left;
}

table.grid tr.item 
{
}

table.grid tr.item-alternating
{
	background-color: #EFEFEF;
}

table.grid tr.item-edit
{
	background-color: #FFE97F;
}

table.grid tr.paging
{
	text-align: center;
}

table.grid tr td
{
	padding: 2px;
}

table.grid tr td img
{
	border: 0;		
}

.grid td
{
	font-family:Arial;
	font-size:12px;
	color:#000;	
	font-weight:normal;
	border:solid 1px #CCCCCC;
}

.grid td a
{
	font-weight:bold; 
	color:Black; 
	text-decoration:underline;	
	display:inline;		
}

.pagination
{
	text-align:center;		
}

.pagination span
{
	color:#000;		
	font-family:Arial;		
}

.pagination a
{
	color:blue;
	font-family:Arial;		
	display:inline;
}

.createContent
{
	display:inline !important;
	color:Blue !important;
	text-decoration:underline !important;
}

/* ADMIN TABLE */
